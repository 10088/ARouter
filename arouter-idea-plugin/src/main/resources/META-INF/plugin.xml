<idea-plugin>
    <id>arouter-roadsign</id>
    <name>ARouter Helper</name>
    <version>1.0.1</version>
    <vendor email="zhilong.liu@aliyun.com" url="https://www.alibaba.com">Alibaba</vendor>
    <idea-version since-build="162.0" />

    <description><![CDATA[
    Support to track navigation target.
    ]]></description>

    <depends optional="true">org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <codeInsight.lineMarkerProvider implementationClass="com.alibaba.android.arouter.idea.extensions.NavigationLineMarker"
            language="JAVA" />
        <codeInsight.lineMarkerProvider implementationClass="com.alibaba.android.arouter.idea.extensions.NavigationLineMarker"
            language="kotlin" />
    </extensions>
</idea-plugin>